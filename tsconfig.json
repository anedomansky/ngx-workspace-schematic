{
  "compilerOptions": {
    "outDir": "./src",
    "baseUrl": "./",
    "declaration": false,
    "module": "commonjs",
    "moduleResolution": "node",
    "noEmitOnError": true,
    "strict": true,
    "skipDefaultLibCheck": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "target": "ES2022"
  },
  "include": [],
  "exclude": [],
  "references": [
    {
      "path": "./tsconfig.app.json"
    },
    {
      "path": "./tsconfig.spec.json"
    },
    {
      "path": "./tsconfig.editor.json"
    }
  ]
}
